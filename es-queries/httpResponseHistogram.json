{
  "index": "filebeat-*",
  "body":{  
  "aggs": {
    "Timestamp": {
      "date_histogram": {
        "field": "@timestamp",
        "calendar_interval": "1m",
        "time_zone": "America/New_York",
        "min_doc_count": 1
      },
      "aggs": {
        "HTTPResponse": {
          "terms": {
            "field": "http.response.status_code",
            "order": {
              "_count": "desc"
            },
            "size": 5
          }
        }
      }
    }
  },
  "size": 0,
  "script_fields": {},
  "stored_fields": [
    "*"
  ],
  "runtime_mappings": {},
  "query": {
    "bool": {
      "must": [],
      "filter": [
        {
          "range": {
            "@timestamp": {
              "format": "strict_date_optional_time",
              "gte": "now-1h/h",
              "lte": "now/h"
            }
          }
        }
      ],
      "should": [],
      "must_not": []
    }
  }
}
}